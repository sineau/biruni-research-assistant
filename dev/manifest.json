{
  "manifest_version": 2,
  "name": "Biruni Research Assistant",
  "version": "0.1.0",
  "background": {
    "scripts": [
      "background.bundle.js",
      "store.bundle.js"
    ]
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "./icons/16.png",
      "32": "./icons/32.png",
      "64": "./icons/64.png"
    }
  },
  "icons": {
    "16": "./icons/16.png",
    "48": "./icons/48.png",
    "96": "./icons/96.png",
    "128": "./icons/128.png"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "contextMenus",
    "bookmarks"
  ]
}
